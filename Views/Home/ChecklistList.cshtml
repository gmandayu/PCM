@{ checklistList.CloseRecordset(); }
@if (!checklistList.IsExport()) {
<div class="card-footer ew-grid-lower-panel">
@if (!checklistList.IsGridAdd && !(checklistList.IsGridEdit && checklistList.ModalGridEdit) && !checklistList.IsMultiEdit) {
@Html.Raw(checklistList.Pager.Render())
}
<div class="ew-list-other-options">
@checklistList.OtherOptions.RenderBody("bottom")
</div>
</div>
}
</div><!-- /.ew-grid -->
} else { // Show other options
<div class="ew-list-other-options">
