ew.deepAssign(ew.vars, { tables: { Checklist: currentTable } });
var currentForm;
var fChecklistsrch, currentSearchForm, currentAdvancedSearchForm;
loadjs.ready(["wrapper", "head"], function () {
    let $ = jQuery,
        fields = currentTable.fields;

    // Form object for search
    let form = new ew.FormBuilder()
        .setId("fChecklistsrch")
        .setPageId("list")
@if (checklistList.UseAjaxActions) {
        <text>.setSubmitWithFetch(true)</text>
}

        // Add fields
        .setFields([

            ["EmployeeStatus", [], fields.EmployeeStatus.isInvalid],
            ["IndividualCodeNumber", [], fields.IndividualCodeNumber.isInvalid],
            ["FullName", [], fields.FullName.isInvalid],
            ["RequiredPhoto", [], fields.RequiredPhoto.isInvalid],
            ["VisaPhoto", [], fields.VisaPhoto.isInvalid],
            ["RankAppliedFor", [], fields.RankAppliedFor.isInvalid],
            ["WillAcceptLowRank", [], fields.WillAcceptLowRank.isInvalid],
            ["AvailableFrom", [], fields.AvailableFrom.isInvalid],
            ["AvailableUntil", [], fields.AvailableUntil.isInvalid],
            ["CreatedBy", [], fields.CreatedBy.isInvalid],
            ["CreatedDateTime", [], fields.CreatedDateTime.isInvalid],
            ["LastUpdatedBy", [], fields.LastUpdatedBy.isInvalid],
            ["LastUpdatedDateTime", [], fields.LastUpdatedDateTime.isInvalid]
        ])
        // Validate form
        .setValidate(
            async function () {
                if (!this.validateRequired)
                    return true; // Ignore validation
                let fobj = this.getForm();

                // Validate fields
                if (!this.validateFields())
                    return false;

                // Call Form_CustomValidate event
                if (!(await this.customValidate?.(fobj) ?? true)) {
                    this.focus();
                    return false;
                }
                return true;
            }
        )

        // Form_CustomValidate
        .setCustomValidate(
            function(fobj) { // DO NOT CHANGE THIS LINE!
                    // Your custom validation code here, return false if invalid.
                    return true;
                }
        )

        // Use JavaScript validation or not
        .setValidateRequired(ew.CLIENT_VALIDATE)

        // Dynamic selection lists
        .setLists({
            "EmployeeStatus": @Html.Raw(checklistList.EmployeeStatus.ToClientList(checklistList)),
            "IndividualCodeNumber": @Html.Raw(checklistList.IndividualCodeNumber.ToClientList(checklistList)),
            "FullName": @Html.Raw(checklistList.FullName.ToClientList(checklistList)),
            "RequiredPhoto": @Html.Raw(checklistList.RequiredPhoto.ToClientList(checklistList)),
            "VisaPhoto": @Html.Raw(checklistList.VisaPhoto.ToClientList(checklistList)),
            "RankAppliedFor": @Html.Raw(checklistList.RankAppliedFor.ToClientList(checklistList)),
            "WillAcceptLowRank": @Html.Raw(checklistList.WillAcceptLowRank.ToClientList(checklistList)),
            "AvailableFrom": @Html.Raw(checklistList.AvailableFrom.ToClientList(checklistList)),
            "AvailableUntil": @Html.Raw(checklistList.AvailableUntil.ToClientList(checklistList)),
            "CreatedBy": @Html.Raw(checklistList.CreatedBy.ToClientList(checklistList)),
            "CreatedDateTime": @Html.Raw(checklistList.CreatedDateTime.ToClientList(checklistList)),
            "LastUpdatedBy": @Html.Raw(checklistList.LastUpdatedBy.ToClientList(checklistList)),
            "LastUpdatedDateTime": @Html.Raw(checklistList.LastUpdatedDateTime.ToClientList(checklistList)),
        })

        // Filters
        .setFilterList(@Html.Raw(await checklistList.GetFilterList()))
        .build();
    window[form.id] = form;
    currentSearchForm = form;
    loadjs.done(form.id);
});
</script>
