@if (!mtCurrencyView.IsExport()) {
<script>
var currentTable = @Html.Raw(ConvertToJson(mtCurrencyView.ToClientVar()));
ew.deepAssign(ew.vars, { tables: { MTCurrency: currentTable } });
var currentPageID = ew.PAGE_ID = "view";
var currentForm;
var fMTCurrencyview;
loadjs.ready(["wrapper", "head"], function () {
    let $ = jQuery;
    let fields = currentTable.fields;

    // Form object
    let form = new ew.FormBuilder()
        .setId("fMTCurrencyview")
        .setPageId("view")
        .build();
    window[form.id] = form;
    currentForm = form;
    loadjs.done(form.id);
});
</script>
}
