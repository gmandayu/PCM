                            let valid = false;
                            if (year >= 1900 && year <= 9999 && month >= 0 && day >= 1 && day <= 31) {
                                if (month === 1) {
                                    valid = day <= (((year % 4 === 0 && year % 100 !== 0) || year % 400 === 0) ? 29 : 28);
