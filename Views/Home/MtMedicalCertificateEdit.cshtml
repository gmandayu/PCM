var currentTable = @Html.Raw(ConvertToJson(mtMedicalCertificateEdit.ToClientVar()));
ew.deepAssign(ew.vars, { tables: { MTMedicalCertificate: currentTable } });
var currentPageID = ew.PAGE_ID = "edit";
var currentForm;
